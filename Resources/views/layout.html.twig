{# Resources/views/layout.html.twig #}
{% extends "@Bootstrap/layout.html.twig" %}
{% block bootstrapHeadStyles %}
    {{ parent() }}
    {% include "@JQueryDataTables/include/styles.html.twig" %}
    <style type="text/css">
        .content {
            margin: 70px 0 0;
        }
    </style>
    {% block jQueryDataTablesHeadStyles %}
    {% endblock %}
{% endblock %}
{% block bootstrapHeadTitle %}webeweb/jqeury-detatabales-bundle{% endblock %}
{% block bootstrapBodyContent %}
    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button class="navbar-toggle collapsed" type="button" data-target="#bs-edmbundle-navbar-collapse" data-toggle="collapse" aria-expanded="false">
                    <span class="sr-only">{{ "label.toggle_navigation"|trans({}, "BootstrapBundle") }}</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">jQuery DataTables bundle</a>
            </div>
        </div>
    </nav>
    <section class="content">
        <div class="container-fluid">
            {% block jQueryDataTablesBodyContent %}
            {% endblock %}
        </div>
    </section>
{% endblock %}
{% block bootstrapBodyScripts %}
    {{ parent() }}
    {% include "@JQueryDataTables/include/scripts.html.twig" %}
    {% block jQueryDataTablesBodyScripts %}
    {% endblock %}
{% endblock %}