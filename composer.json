{
    "name": "webeweb/jquery-datatables-bundle",
    "type": "symfony-bundle",
    "description": "Intregate jQuery Datatable with Symfony 2",
    "keywords": ["webeweb", "bundle", "symfony", "jquery", "datatable"],
    "homepage": "http://github.com/webeweb/jquery-datatables-bundle",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "webeweb",
            "homepage": "http://github.com/webeweb/"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "doctrine/orm": "^2.2",
        "symfony/asset": "^2.6|^3.0",
        "symfony/framework-bundle": "^2.6|^3.0",
        "symfony/monolog-bundle": "^2.6|^3.0",
        "twig/twig": "^1.0||^2.0",
        "webeweb/bootstrap-bundle": "~1.0@dev",
        "webeweb/core-library": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*",
        "doctrine/doctrine-bundle": "^1.0",
        "sensio/framework-extra-bundle": "^2.6|^3.0",
        "symfony/browser-kit": "^2.6|^3.0",
        "symfony/security-bundle": "^2.6|^3.0",
        "symfony/templating": "^2.6|^3.0",
        "symfony/twig-bundle": "^2.6|^3.0"
    },
    "support": {
        "issues": "http://github.com/webeweb/jquery-datatables-bundle/issues"
    },
    "autoload": {
        "psr-4": {
            "WBW\\Bundle\\JQuery\\DataTablesBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
